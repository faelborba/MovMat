<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.movmat">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_movmat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AtividadeCalcular"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Ajuda"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".TelaRelatorio"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ConfereResultado"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TelaDesafio"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SelecionaDesafio"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TelaConfiguracao"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>