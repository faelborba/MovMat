                android:textColor="#FFFFFF"
                android:textSize="18sp" />

        </TableRow>

        <TableRow
            android:id="@+id/row5"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/celContagem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="Contagem"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/contagemAcertou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/contagemErrou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/contagemTotal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
