                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/subtraiTotal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:id="@+id/row3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/celMultiplicar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="Multiplicar"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/multiplicaAcertou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/multiplicaErrou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/multiplicaTotal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:id="@+id/row4"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/celDividir"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="Dividir"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/divisaoAcertou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/divisaoErrou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
                android:padding="5dp"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/divisaoTotal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_cor"
